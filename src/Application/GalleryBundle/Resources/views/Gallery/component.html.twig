{% if images|length > 0 %}
<div class="blog-photo-stream margin-bottom-20">
    <h2>{{'page.gallery.block_title'|trans}}</h2>
    <ul class="list-unstyled">
        {% for image in images %}
        {% set src = vich_uploader_asset(image, 'file') %}
        <li><a href="{{path('web_gallery', {id : image.id})}}"><img src="{{src|imagine_filter('gallery_image_block')}}" alt="{{image.name}}"></a></li>
        {% endfor %}
    </ul>
</div>
{% endif %}
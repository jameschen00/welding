
<!DOCTYPE html>
<!--[if IE 8]> <html lang="en" class="ie8 no-js"> <![endif]-->
<!--[if IE 9]> <html lang="en" class="ie9 no-js"> <![endif]-->
<!--[if !IE]><!--> <html lang="en" class="no-js"> <!--<![endif]-->
<!-- BEGIN HEAD -->
<head>
    <meta charset="utf-8" />
    <title>{% block title %}Test Application{% endblock %}</title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta content="width=device-width, initial-scale=1.0" name="viewport" />
    <meta content="" name="description" />
    <meta content="" name="author" />
    <meta name="MobileOptimized" content="320">
    <!-- BEGIN GLOBAL MANDATORY STYLES -->
    <link href="/bundles/applicationadmin/plugins/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css"/>
    <link href="/bundles/applicationadmin/plugins/bootstrap/css/bootstrap.min.css" rel="stylesheet" type="text/css"/>
    <link href="/bundles/applicationadmin/plugins/uniform/css/uniform.default.css" rel="stylesheet" type="text/css"/>
    <!-- END GLOBAL MANDATORY STYLES -->
    <!-- BEGIN PAGE LEVEL PLUGIN STYLES -->
    <link href="/bundles/applicationadmin/plugins/gritter/css/jquery.gritter.css" rel="stylesheet" type="text/css"/>
    <link href="/bundles/applicationadmin/plugins/bootstrap-daterangepicker/daterangepicker-bs3.css" rel="stylesheet" type="text/css" />
    <link href="/bundles/applicationadmin/plugins/fullcalendar/fullcalendar/fullcalendar.css" rel="stylesheet" type="text/css"/>
    <link href="/bundles/applicationadmin/plugins/jqvmap/jqvmap/jqvmap.css" rel="stylesheet" type="text/css"/>
    <link href="/bundles/applicationadmin/plugins/jquery-easy-pie-chart/jquery.easy-pie-chart.css" rel="stylesheet" type="text/css"/>
    <!-- END PAGE LEVEL PLUGIN STYLES -->
    <!-- BEGIN THEME STYLES -->
    <link href="/bundles/applicationadmin/css/style-metronic.css" rel="stylesheet" type="text/css"/>
    <link href="/bundles/applicationadmin/css/style.css" rel="stylesheet" type="text/css"/>
    <link href="/bundles/applicationadmin/css/style-responsive.css" rel="stylesheet" type="text/css"/>
    <link href="/bundles/applicationadmin/css/plugins.css" rel="stylesheet" type="text/css"/>
    <link href="/bundles/applicationadmin/css/pages/tasks.css" rel="stylesheet" type="text/css"/>
    <link href="/bundles/applicationadmin/css/themes/default.css" rel="stylesheet" type="text/css" id="style_color"/>
    <link href="/bundles/applicationadmin/css/custom.css" rel="stylesheet" type="text/css"/>

    {% block stylesheets %}
    {% stylesheets output='assets/admin.css' filter='cssrewrite'
    '@widget_css'
    'public/js/gallery/css/blueimp-gallery.min.css'
    %}
    <link rel="stylesheet" href="{{ asset_url }}" type="text/css" />
    {% endstylesheets %}
    {% endblock %}


    <!-- END THEME STYLES -->
    <link rel="shortcut icon" href="favicon.ico" />
</head>
<!-- END HEAD -->
<!-- BEGIN BODY -->
<body class="page-header-fixed">
    <!-- BEGIN HEADER -->
    {{ render(controller('ApplicationAdminBundle:Default:header')) }}
    <!-- END HEADER -->

    <div class="clearfix"></div>

    <!-- BEGIN CONTAINER -->
    <div class="page-container">
        <!-- BEGIN SIDEBAR -->
        {{ render(controller('ApplicationAdminBundle:Default:menu')) }}
        <!-- END SIDEBAR -->

        <!-- BEGIN PAGE -->
        <div class="page-content">
            <!-- BEGIN PAGE HEADER-->
            <div class="row">
                <div class="col-md-12">
                    <h3 class="page-title">
                        {% block page_header %}Page header{% endblock %}
                    </h3>
                    {{ wo_render_breadcrumbs({ viewTemplate: "ApplicationAdminBundle::breadcrumbs.html.twig" }) }}
                </div>
            </div>
            <!-- END PAGE HEADER-->

            <div class="row">
                <div class="col-md-12">
                    <div class="tabbable tabbable-custom tabbable-full-width">
                        <div class="page-toolbar">
                            <div class="pull-right">
                                {% block page_controll %}{% endblock %}
                            </div>
                            <div class="clearfix"></div>
                        </div>
                        {% block body %}{% endblock %}
                    </div>
                </div>
            </div>
        </div>
        <!-- END PAGE -->
    </div>
    <!-- END CONTAINER -->

    <!-- BEGIN FOOTER -->
    {{ render(controller('ApplicationAdminBundle:Default:footer')) }}
    <!-- END FOOTER -->

    <!-- GALLERY -->
    {% include 'ApplicationAdminBundle::gallery.html.twig' %}
    <!-- END GALLERY -->

    <!-- BEGIN JAVASCRIPTS(Load javascripts at bottom, this will reduce page load time) -->

    <!-- BEGIN CORE PLUGINS -->
    <!--[if lt IE 9]>
    <script src="/bundles/applicationadmin/plugins/respond.min.js"></script>
    <script src="/bundles/applicationadmin/plugins/excanvas.min.js"></script>
    <![endif]-->
    <script src="/bundles/applicationadmin/plugins/jquery-1.10.2.min.js" type="text/javascript"></script>
    <script src="/bundles/applicationadmin/plugins/jquery-migrate-1.2.1.min.js" type="text/javascript"></script>
    <!-- IMPORTANT! Load jquery-ui-1.10.3.custom.min.js before bootstrap.min.js to fix bootstrap tooltip conflict with jquery ui tooltip -->
    <script src="/bundles/applicationadmin/plugins/jquery-ui/jquery-ui-1.10.3.custom.min.js" type="text/javascript"></script>
    <script src="/bundles/applicationadmin/plugins/bootstrap/js/bootstrap.min.js" type="text/javascript"></script>
    <script src="/bundles/applicationadmin/plugins/bootstrap-hover-dropdown/twitter-bootstrap-hover-dropdown.min.js" type="text/javascript" ></script>
    <script src="/bundles/applicationadmin/plugins/jquery-slimscroll/jquery.slimscroll.min.js" type="text/javascript"></script>
    <script src="/bundles/applicationadmin/plugins/jquery.blockui.min.js" type="text/javascript"></script>
    <script src="/bundles/applicationadmin/plugins/jquery.cookie.min.js" type="text/javascript"></script>
    <script src="/bundles/applicationadmin/plugins/uniform/jquery.uniform.min.js" type="text/javascript" ></script>
    <!-- END CORE PLUGINS -->

    <script type="text/javascript" src="/public/js/tinymce/jquery.tinymce.min.js"></script>
    <script type="text/javascript" src="/public/js/tinymce/tinymce.min.js"></script>

    <!-- BEGIN PAGE LEVEL PLUGINS -->
    <script src="/bundles/applicationadmin/plugins/flot/jquery.flot.js" type="text/javascript"></script>
    <script src="/bundles/applicationadmin/plugins/flot/jquery.flot.resize.js" type="text/javascript"></script>
    <script src="/bundles/applicationadmin/plugins/jquery.pulsate.min.js" type="text/javascript"></script>
    <script src="/bundles/applicationadmin/plugins/bootstrap-daterangepicker/moment.min.js" type="text/javascript"></script>
    <script src="/bundles/applicationadmin/plugins/bootstrap-daterangepicker/daterangepicker.js" type="text/javascript"></script>
    <script src="/bundles/applicationadmin/plugins/gritter/js/jquery.gritter.js" type="text/javascript"></script>
    <!-- IMPORTANT! fullcalendar depends on jquery-ui-1.10.3.custom.min.js for drag & drop support -->
    <script src="/bundles/applicationadmin/plugins/fullcalendar/fullcalendar/fullcalendar.min.js" type="text/javascript"></script>
    <script src="/bundles/applicationadmin/plugins/jquery-easy-pie-chart/jquery.easy-pie-chart.js" type="text/javascript"></script>
    <script src="/bundles/applicationadmin/plugins/jquery.sparkline.min.js" type="text/javascript"></script>
    <!-- END PAGE LEVEL PLUGINS -->
    <!-- BEGIN PAGE LEVEL SCRIPTS -->
    <script src="/bundles/applicationadmin/scripts/app.js" type="text/javascript"></script>
    <script src="/bundles/applicationadmin/scripts/index.js" type="text/javascript"></script>
    <script src="/bundles/applicationadmin/scripts/tasks.js" type="text/javascript"></script>
    <!-- END PAGE LEVEL SCRIPTS -->
    <script>
        jQuery(document).ready(function() {
            App.init(); // initlayout and core plugins
        });
    </script>

    {% block javascripts %}
        {% javascripts output='assets/js/backend.js'
            'public/js/gallery/js/jquery.blueimp-gallery.min.js'
        %}
        <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}

    {# tinymce #}
    <script type="text/javascript" src="/public/js/tinymce/jquery.tinymce.min.js"></script>
    <script type="text/javascript" src="/public/js/tinymce/tinymce.min.js"></script>
    {% endblock %}
    <!-- END JAVASCRIPTS -->
</body>
<!-- END BODY -->

</html>


